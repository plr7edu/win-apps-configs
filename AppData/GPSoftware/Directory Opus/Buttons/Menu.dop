<?xml version="1.0" encoding="UTF-8"?>
<toolbar>
	<buttons backcol="none" display="label" textcol="none" type="menu">
		<button backcol="none" display="label" label_pos="right" textcol="none" type="menu">
			<label>&amp;File</label>
			<button backcol="none" display="both" hotkey="ctrl+E" hotkey_label="yes" label_pos="right" separate="yes" textcol="none">
				<label>&amp;Open New Lister</label>
				<tip>Open a new Lister</tip>
				<icon1>#newlister</icon1>
				<function type="normal">
					<instruction>Go NEW</instruction>
				</function>
			</button>
			<button backcol="none" display="both" label_pos="right" separate="yes" textcol="none">
				<label>&amp;Undo Close Lister</label>
				<tip>Reopen the last closed Lister</tip>
				<icon1>#newlister</icon1>
				<function type="normal">
					<instruction>Go UNDOCLOSELISTER</instruction>
				</function>
			</button>
			<button backcol="none" display="both" hotkey_label="yes" label_pos="right" textcol="none">
				<label>&amp;Close Lister</label>
				<tip>Close the current Lister</tip>
				<hotkeys>
					<key>alt+f4</key>
					<key>shift+escape</key>
				</hotkeys>
				<icon1>#closelister</icon1>
				<function type="normal">
					<instruction>Close</instruction>
				</function>
			</button>
			<button backcol="none" display="both" label_pos="right" textcol="none">
				<label>&amp;Exit Directory Opus</label>
				<tip>Exit Directory Opus</tip>
				<icon1>#exit</icon1>
				<function type="normal">
					<instruction>Close PROGRAM=confirm</instruction>
				</function>
			</button>
		</button>
		<button backcol="none" display="label" label_pos="right" textcol="none" type="menu">
			<label>&amp;Edit</label>
			<button backcol="none" display="both" hotkey="ctrl+Z" hotkey_label="yes" label_pos="right" textcol="none">
