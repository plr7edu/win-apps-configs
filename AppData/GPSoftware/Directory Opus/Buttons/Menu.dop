				<icon1>#undo2</icon1>
				<function type="normal">
					<instruction>Undo</instruction>
				</function>
			</button>
			<button backcol="none" display="both" hotkey="ctrl+shift+Z" hotkey_label="yes" label_pos="right" textcol="none">
				<label>&amp;Redo</label>
				<tip>Redo the previously undone file action</tip>
				<icon1>#redo2</icon1>
				<function type="normal">
					<instruction>Undo REDO</instruction>
				</function>
			</button>
			<button backcol="none" display="both" label_pos="right" separate="yes" textcol="none" type="menu">
				<label>U&amp;ndo List</label>
